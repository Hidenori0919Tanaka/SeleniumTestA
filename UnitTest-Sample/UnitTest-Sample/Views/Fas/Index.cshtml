@model IEnumerable<UnitTest_Sample.Models.Fa>

@{
//Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    @if (Model.FirstOrDefault() != null)
    {
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.User.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FaName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.CreateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.UpdateDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DeleteFlag)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.User.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FaName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreateDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.UpdateDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DeleteFlag)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.FaId }) |
                        @Html.ActionLink("Details", "Details", new { id = item.FaId }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.FaId })
                    </td>
                </tr>
            }

        </table>
    }
</body>
</html>
@if (HttpContext.Current.IsDebuggingEnabled)
{
    <p>デバッグ</p>
}